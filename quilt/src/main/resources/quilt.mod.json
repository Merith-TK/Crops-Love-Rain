{
  "schema_version": 1,
  "mixin": [
    "cropsloverain.mixins.json",
    "cropsloverain-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "cropsloverain",
    "version": "${version}",
    "metadata": {
      "name": "Crops Love Rain",
      "description": "Makes crops grow faster when it's raining.",
      "contributors": {
        "ThePoultryMan": "Owner"
      },
      "contact": {
        "sources": "https://github.com/ThePoultryMan/Crops-Love-Rain"
      },
      "icon": "assets/cropsloverain/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "io.github.thepoultryman.cropsloverain.quilt.CropsLoveRainQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.20.1"
      },
      {
        "id": "architectury",
        "version": ">=9.1.12"
      }
    ]
  },
  "minecraft": {
    "environment": "*"
  }
}